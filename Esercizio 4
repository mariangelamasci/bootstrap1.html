var n=0;
n=Number(window.prompt("inserisci il numero di giornate"));
var punti_quadra= new Arrey();
var i=0;
var punti=0;
var somma=0;
 while (1<n)
{
punti=Number(windw.prompt("inserisci i punti ottenuti dalla squadra" +i));

punti_squadra(i)=punti; i++;
}
i=0;
docume.write(" i punto servizio le vetture sono: ");
while(i<n)
{
documenti.write(punti_squadra i]+"-"); i++;
} 
i=0
while (i<n)
{
somma=somma+punti_squadra[i]; i++;
}
document.write("<br/>);
document.write("<br/> la somma dei punti contenuti nel vettore e di "+somma );
